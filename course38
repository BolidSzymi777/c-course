#include<iostream>      
#include<cstring>
#include<cmath>
#include<cctype>

using namespace std;





//9. Napisz funkcję minimum, która zwraca mniejszą z dwóch liczb.
int minimum(int a, int b) {
	if (a > b) {
		return b;
	}
	else {
		return a;
	}
	
}
int main() {
	cout << minimum(12, 7);
	return 0;
}
