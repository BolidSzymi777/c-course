#include <iostream>
#include <cstring>
#include <cctype>
using namespace std;

int main() {
    char name[20];
    char name2[20];
    cout << "Podaj wyraz" << endl;
    cin >> name;
    cout << "Podaj drugi wyraz" << endl;
    cin >> name2;
    int cmp = strcmp(name, name2);

    if (cmp == 0)
        cout << "Napishy sa rowne" << endl;
    else if (cmp > 0)
        cout << "Pierwsze slowo jest wieksze leksykograficznie" << endl;
    else cout << "Drugie slowo jesst wieksze leksykograficznie" << endl;
    return 0;
}
