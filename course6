#include<iostream>
#include<cmath>
using namespace std;

int main()
{
	double benzyna, koszt, dystans, spalanie;
	cout << "Podaj cene benzyny" <<endl;
	cin >> benzyna;
	cout << "Podaj dlugosc trasy" <<endl;
	cin >> dystans;
	cout << "Podaj spalanie na 100km" <<endl;
	cin >> spalanie;
	koszt = spalanie * dystans / 100 * benzyna;
	cout << "Koszt podrozy: " << koszt;

	return 0;
}
